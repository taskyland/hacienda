{
  "$schema": "https://biomejs.dev/schemas/1.9.3/schema.json",
  "extends": ["@taskylizard/biome-config", "./.biomelintrc-auto-import.json"],
  "linter": {
    "rules": {
      "suspicious": {
        "noReactSpecificProps": "error"
      },
      "style": {
        "noDefaultExport": "off"
      },
      "nursery": {
        "useSortedClasses": "info"
      }
    }
  }
}
